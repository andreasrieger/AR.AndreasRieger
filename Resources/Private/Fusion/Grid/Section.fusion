prototype(AR.AndreasRieger:Section) < prototype(Neos.Neos:Content) {
  templatePath = 'resource://AR.AndreasRieger/Private/Templates/NodeTypes/Section.html'
  #@context.sectionId = ${q(node).property('sectionId') ? q(node).property('sectionId') : node.identifier}
  attributes {
    class = 'container-fluid py-5'
    id = ${q(node).property('sectionId') ? q(node).property('sectionId') : node.name}
  }
  sectionContent = Neos.Neos:ContentCollection {
    nodePath = 'sectionContent'
  }
}
