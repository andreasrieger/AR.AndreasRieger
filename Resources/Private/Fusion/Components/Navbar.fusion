prototype(Site:Navbar) < prototype(Menu) {
  templatePath = 'resource://AR.KochSanitaetshaus/Private/Templates/Components/Navbar.html'
  entryLevel = 1
  maximumLevels = 2
  attributes {
  	class = 'navbar fixed-top navbar-light bg-light justify-content-end'
  	id = 'main-navbar'
  	role = 'navigation'
  }
  filter = 'Neos.Neos:Document, !AR.KochSanitaetshaus:Reference'
  @cache.entryIdentifier.documentNode = ${site}
}
